# OpenRouter API 配置
OPENROUTER_API_KEY=your_openrouter_api_key_here

# 模型设置
MODEL_NAME=minimax/minimax-01
API_BASE_URL=https://openrouter.ai/api/v1

# 图像分析参数
MAX_IMAGE_SIZE=2048
COMPRESSION_QUALITY=85

# 日志级别
LOG_LEVEL=INFO

# React Native 后端设置
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# 缓存设置
ENABLE_CACHE=true
CACHE_TTL=3600